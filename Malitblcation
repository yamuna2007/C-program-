#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    // Check constraints: 1 <= n <= 9
    if (n > 0 && n < 10) {
        for (int i = 1; i <= n; i++) {
            printf("%d x %d = %d\n", n, i, n * i);
        }
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
